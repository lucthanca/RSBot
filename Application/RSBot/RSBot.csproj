<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>disable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
		<ImplicitUsings>disable</ImplicitUsings>
		<OutputPath>.\..\..\Build</OutputPath>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<Description>A open source bot for Silkroad Online</Description>
		<Copyright>Copyright © 2024, RSBot Team</Copyright>
		<PackageProjectUrl>https://github.com/SDClowen/RSBot</PackageProjectUrl>
		<RepositoryUrl>https://github.com/SDClowen/RSBot</RepositoryUrl>
		<OutputType>WinExe</OutputType>
		<ApplicationIcon>app.ico</ApplicationIcon>
		<AssemblyVersion>2.9.3</AssemblyVersion>
		<ApplicationHighDpiMode></ApplicationHighDpiMode>
		<PlatformTarget>x86</PlatformTarget>
		<UseWPF>False</UseWPF>
	</PropertyGroup>

	<ItemGroup>
	  <Content Include="app.ico" />
	</ItemGroup>
  <ItemGroup>
    <Content Include="..\..\Dependencies\**\*.*">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <DeploymentContent>true</DeploymentContent>
      <Link>Data\%(RecursiveDir)\%(Filename)%(Extension)</Link>
    </Content>
  </ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\Library\RSBot.Core\RSBot.Core.csproj">
		  <Private>True</Private>
		</ProjectReference>
		<ProjectReference Include="..\..\SDUI\SDUI\SDUI.csproj" />
		<ProjectReference Include="..\Vadu.AlphaForm\Vadu.AlphaForm.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Update="Views\MainLegacy.cs">
	    <SubType>Form</SubType>
	  </Compile>
	</ItemGroup>
</Project>